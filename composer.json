{
    "name": "ultrono/laravel-sitemap",
    "description": "PHP 8 sitemap generator for Laravel 8, 9, 10 and 11",
    "keywords": ["laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Original Developer",
            "homepage": "https://darumen.com"
        },
        {
            "name": "Rob Allport",
            "email": "rob@f9web.co.uk",
            "role": "Developer",
            "homepage": "https://f9web.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.4|^10.1"
    },
    "autoload": {
        "psr-0": {
            "Ultrono\\Sitemap": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ultrono\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
